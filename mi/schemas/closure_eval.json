{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MI ClosureEval",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "status",
    "confidence",
    "done_reasons",
    "blocking_issues",
    "ask_user_question",
    "notes"
  ],
  "properties": {
    "status": { "type": "string", "enum": ["done", "not_done", "blocked"] },
    "confidence": { "type": "number", "minimum": 0, "maximum": 1 },
    "done_reasons": { "type": "array", "items": { "type": "string" } },
    "blocking_issues": { "type": "array", "items": { "type": "string" } },
    "ask_user_question": { "type": "string" },
    "notes": { "type": "string" }
  }
}

